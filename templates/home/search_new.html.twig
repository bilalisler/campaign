<!-- Listings -->
<section id="ElemoListing_with_map">
    <div class="ElemoListing_map">
        <div id="map-container">
            <div id="map" style="position: relative; overflow: hidden;">
                <div style="height: 100%; width: 100%; position: absolute; top: 0px; left: 0px; background-color: rgb(229, 227, 223);">
                    <div class="gm-style" style="position: absolute; z-index: 0; left: 0px; top: 0px; height: 100%; width: 100%; padding: 0px; border-width: 0px; margin: 0px;">

                        <link rel="stylesheet" href="{{ asset('assets/css/map_item.css') }}">

                        <div id="map"></div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="search_wrap">
        <div class="search_form">
            <form action="http://themes.webmasterdriver.net/ElemoListing/listing-grid-map.html" method="get">
                <div class="form-group">
                    <div class="select">
                        <select class="form-control">
                            <option>What are you looking for?</option>
                            <option>Real Estate</option>
                            <option>Restaurant</option>
                            <option>Real Estate</option>
                            <option>Health &amp; Fitness</option>
                            <option>Beauty &amp; Spas</option>
                            <option>Hotels &amp; Travel</option>
                            <option>Automotive</option>
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" placeholder="Locaton">
                </div>
                <div class="btn_group">
                    <input type="submit" value="Search" class="btn btn-block">
                </div>
            </form>
        </div>
    </div>

    <div class="container">
        <div class="listing_header">
            <h5>
                {{ pagination.getTotalItemCount }} Sonuç bulundu
            </h5>
        </div>

        <div class="row">
            {% for product in pagination %}

                <div class="col-md-6 grid_col show_listing">
                    <div class="items-list listing_wrap" data-post-id="1">
                        <div class="listing_img">
                            <span class="like_post"><i class="fa fa-bookmark-o"></i></span>
                            <div class="listing_cate">
                                <span class="cate_icon"><a href="{{ path("product_detail",{'categorySlug':product.category.categorySlug,'slug':product.slug}) }}"><img src="{{ asset('assets/icon/category-icon5.png') }}" alt="icon-img"></a></span>
                                <span class="listing_like"><a href="{{ path("product_detail",{'categorySlug':product.category.categorySlug,'slug':product.slug}) }}"><i class="fa fa-heart-o"></i></a></span>
                            </div>
                            <a href="{{ path("product_detail",{'categorySlug':product.category.categorySlug,'slug':product.slug}) }}">
                                {% if (product.productImages |fetchAvatarImage) == null %}
                                    {% set productImage = 'assets/images/search/listing_img2.jpg' %}
                                {% else %}
                                    {% set productImage = product_image_path ~ '/' ~ (product.productImages |fetchAvatarImage) %}
                                {% endif %}

                                <img src="{{ asset(productImage) }}" width="483" height="286" alt="image">
                            </a>
                        </div>
                        <div class="listing_info">
                            <div class="post_category">
                                <a href="{{ path("category_list",{'slug': product.category.categorySlug}) }}">{{ product.category.categoryName }}</a>
                            </div>
                            <h4><a href="{{ path("product_detail",{'categorySlug':product.category.categorySlug,'slug':product.slug}) }}">{{ product.name }}</a></h4>
                            {#<p>5-star hotel with restaurant, Deluxe Rooms.</p>#}


                            <div class="listing_review_info">
                                <p>
                                    <span class="review_score"> {{ (product.comments |getProductRating |number_format(1)) ~ '/' ~ constant('App\\Entity\\ProductComments::MAX_RATING')|number_format(1) }} </span>
                                    <span class="product-rating">
                                        {{ product.comments |getProductRating(1) |raw }}
                                    </span>
                                    ({{ product.comments |length }} Yorum)
                                </p>
                                <p class="listing_map_m"><i class="fa fa-map-marker"></i> {{ product.shop.city.name |capitalize ~'/'~ product.shop.town.name |capitalize }}
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>
        <nav class="pagination_nav">
            <div class="navigation">
                {{ knp_pagination_render(pagination) }}
            </div>
        </nav>
    </div>
</section>
<!-- /Listings -->

